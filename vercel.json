{
    "version": 2,
    "builds": [
      {
        "src": "build/web/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/build/web/$1"
      },
      {
        "src": "/(.+\\.[a-z]+)$",
        "dest": "/build/web/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/build/web/index.html"
      }
    ]
  }